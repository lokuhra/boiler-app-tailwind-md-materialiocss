{
  "name": "Proyecto ux",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "^8.5.1",
    "cssnano": "^3.10.0",
    "glhd-tailwindcss-transitions": "^0.3.0",
    "postcss-all-link-colors": "^2.0.0",
    "postcss-cli": "^5.0.0",
    "postcss-grid-kiss": "^1.4.2",
    "postcss-responsive-font": "^1.0.3",
    "prettier": "^1.13.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-md": "^1.3.2",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "tailwindcss": "^0.5.3",
    "tailwindcss-blend-mode": "^0.1.0",
    "tailwindcss-cursor-extended": "^0.1.0"
  },
  "scripts": {
    "b:grid-kiss": "postcss styles/grid-kiss/grid.css --output static/styles/grid.css --use postcss-grid-kiss",
    "b:css": "postcss styles/tailwind/index.css -o static/styles/tailwind.css",
    "w:css": "postcss styles/tailwind/index.css -o static/styles/tailwind.css -w",
    "start": "npm run b:css && react-scripts start",
    "build": "npm run b:css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
